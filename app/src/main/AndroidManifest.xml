<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.todoapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_icon_sticky_note"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_icon_sticky_note_round"
        android:supportsRtl="true"
        android:name=".OfflinePersistence"
        android:theme="@style/Theme.TodoApp">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.TodoApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TaskItemsActivity" />
        <activity android:name=".TaskListActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MyProfileActivity" />
    </application>

</manifest>